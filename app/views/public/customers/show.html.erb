<div class="container">
  <div class="row">
    <div class="mx-auto">
      <body>
        <h2>マイページ</h2>
        <h2>登録情報</h2>
        <%= link_to '編集', customer_edit_path, class: "btn btn-outline-secondary" %>
        <table class="table">
          <tr>
            <td>氏名</td>
            <td><%= current_customer.last_name %><%= current_customer.first_name %></td>
          </tr>
          <tr>
            <td>カナ</td>
            <td><%= current_customer.last_name_kana %><%= current_customer.first_name_kana %></td>
          </tr>
          <tr>
            <td>郵便番号</td>
            <td><%= current_customer.postal_code %></td>
          </tr>
          <tr>
            <td>住所</td>
            <td><%= current_customer.address %></td>
          </tr>
          <tr>
            <td>電話番号</td>
            <td><%= current_customer.telephone_number %></td>
          </tr>
          <tr>
            <td>メールアドレス</td>
            <td><%= current_customer.email %></td>
          </tr>
        </table>
        <h2>配送先</h2>
        <%= link_to '一覧を見る', addres_path, class: "btn btn-outline-secondary" %>
        <h2>注文履歴</h2>
        <%= link_to '一覧を見る', class: "btn btn-outline-secondary" %>
      </body>
    </div>
  </div>
</div>